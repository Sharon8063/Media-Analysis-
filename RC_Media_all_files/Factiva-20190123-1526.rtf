\par\par  \trowd\trgaph15\trqc\trleft-15 \clvertalt\clpadl15\clpadr-15 \cellx720\clvertalt\clpadl15\clpadr-15 \cellx9072\pard \qr \li0\ri0\widctlpar\intbl\aspalpha\aspnum\faauto\adjustright\rin0\lin0 \b SE\b0\cell \pard \ql \li0\ri0\widctlpar\intbl\aspalpha\aspnum\faauto\adjustright\rin0\lin0 \uc2 Australian Financial Review Magazine\cell \pard \ql \li0\ri0\widctlpar\intbl\aspalpha\aspnum\faauto\adjustright\rin0\lin0 {  \trowd\trgaph15\trqc\trleft-15 \clvertalt\clpadl15\clpadr-15 \cellx720\clvertalt\clpadl15\clpadr-15 \cellx9072\row}{\*\bkmkstart toc35}{\*\bkmkend toc35} \trowd\trgaph15\trqc\trleft-15 \clvertalt\clpadl15\clpadr-15 \cellx720\clvertalt\clpadl15\clpadr-15 \cellx9072\pard \qr \li0\ri0\widctlpar\intbl\aspalpha\aspnum\faauto\adjustright\rin0\lin0 \b HD\b0\cell \pard \ql \li0\ri0\widctlpar\intbl\aspalpha\aspnum\faauto\adjustright\rin0\lin0 \b \uc2 CULTURAL POWER\b0\cell \pard \ql \li0\ri0\widctlpar\intbl\aspalpha\aspnum\faauto\adjustright\rin0\lin0 {  \trowd\trgaph15\trqc\trleft-15 \clvertalt\clpadl15\clpadr-15 \cellx720\clvertalt\clpadl15\clpadr-15 \cellx9072\row} \trowd\trgaph15\trqc\trleft-15 \clvertalt\clpadl15\clpadr-15 \cellx720\clvertalt\clpadl15\clpadr-15 \cellx9072\pard \qr \li0\ri0\widctlpar\intbl\aspalpha\aspnum\faauto\adjustright\rin0\lin0 \b BY\b0\cell \pard \ql \li0\ri0\widctlpar\intbl\aspalpha\aspnum\faauto\adjustright\rin0\lin0 \uc2 BROOK TURNER \cell \pard \ql \li0\ri0\widctlpar\intbl\aspalpha\aspnum\faauto\adjustright\rin0\lin0 {  \trowd\trgaph15\trqc\trleft-15 \clvertalt\clpadl15\clpadr-15 \cellx720\clvertalt\clpadl15\clpadr-15 \cellx9072\row} \trowd\trgaph15\trqc\trleft-15 \clvertalt\clpadl15\clpadr-15 \cellx720\clvertalt\clpadl15\clpadr-15 \cellx9072\pard \qr \li0\ri0\widctlpar\intbl\aspalpha\aspnum\faauto\adjustright\rin0\lin0 \b WC\b0\cell \pard \ql \li0\ri0\widctlpar\intbl\aspalpha\aspnum\faauto\adjustright\rin0\lin0 \uc2 4,339 \uc2 words\cell \pard \ql \li0\ri0\widctlpar\intbl\aspalpha\aspnum\faauto\adjustright\rin0\lin0 {  \trowd\